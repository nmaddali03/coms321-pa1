package pa1;

public class insertionSort {

	public void fill(int[] addr, int length) {
		int i = length;
		int j = length;
		for(i = 0, j=length; i<length; i++, j--) {
			addr[i]=j;
		}
	}
	/*
	 *  // X0 = addr
		// X1 = length
		fill:
			SUB X9, X1, XZR
			//if X1 = 0, end
			CBZ X9, end_fill
			STUR X1, [X0, #0]
			SUBI X1, X1, #1
			ADDI X0, X0, #8
			B fill
		end_fill:
			BR LR
	 */
	
}
